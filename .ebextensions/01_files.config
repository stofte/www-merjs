files:
    "/etc/nginx/conf.d/websocket" :
        mode: "000755"
        owner: root
        owner: root
        content: |
            location /ws/ {
                proxy_pass http://merjs.dk;
                proxy_http_version 1.1;
                proxy_set_header Upgrade $http_upgrade;
                proxy_set_header Connection "upgrade";
            }